{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "中药材大全",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/shoppingCart/shoppingCart",
			"style": {
				"navigationBarTitleText": "购物车",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "用户主页",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/content/content",
			"style": {
				"navigationBarTitleText": "商品详情",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/map/map",
			"style": {
				"navigationBarTitleText": "地图",
				"navigationStyle": "custom"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "中药材大全",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"easycom": {
		"autoscan": true,
			"custom": {
			  "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			  "^uni-(.*)": "@/uni_modules/uni-$1/components/uni-$1/uni-$1.vue",
			  "^my-(.*)": "@/components/my-$1/my-$1.vue"
			}
	},
	"path": "pages/index/index", "style": { "onReachBottomDistance": 50 },
	"uniIdRouter": {}
}
