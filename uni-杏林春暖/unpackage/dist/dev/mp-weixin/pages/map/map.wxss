.back{display:flex;position:fixed;top:100rpx;left:20rpx;z-index:1000;background-color:#f4f4f4;border-radius:50%;width:60rpx;height:60rpx;justify-content:center;align-items:center;box-shadow:0 2rpx 10rpx rgba(0,0,0,.1)}.back-image{width:40rpx;height:40rpx}.back:active{transform:scale(.95);background-color:rgba(240,240,240,.9)}.map-container{width:100%;height:100vh;position:fixed;top:0;left:0}.locate-btn{position:fixed;right:30rpx;bottom:250rpx;width:90rpx;height:90rpx;background:#fff;border-radius:50%;box-shadow:0 0 10px rgba(0,0,0,.2);display:flex;justify-content:center;align-items:center;z-index:100}.locate-icon{width:50rpx;height:50rpx}.address-bar{position:fixed;bottom:140rpx;left:30rpx;right:30rpx;background:rgba(255,255,255,.9);padding:25rpx 30rpx;border-radius:16rpx;box-shadow:0 4rpx 20rpx rgba(0,0,0,.1);display:flex;align-items:center;z-index:100}.address-icon{width:40rpx;height:40rpx;margin-right:15rpx}.address-text{font-size:32rpx;color:#333;flex:1}.map-watermark{position:fixed;bottom:30rpx;left:0;right:0;display:flex;flex-direction:column;align-items:center;z-index:50}.map-watermark text{font-size:24rpx;color:rgba(0,0,0,.5);line-height:1.5}
